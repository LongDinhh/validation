{
  "name": "longdinhh/validation",
  "type": "library",
  "description": "A re-write of rakit/validation providing Laravel like validation as a standalone library",
  "keywords": [
    "php",
    "validation",
    "symfony"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dave Redfern",
      "email": "info@somnambulist.tech"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-mbstring": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.5",
    "doctrine/dbal": "~3.1.0",
    "ramsey/uuid": "~4.1",
    "symfony/var-dumper": "~5.3"
  },
  "autoload": {
    "psr-4": {
      "Coccoc\\Validation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Coccoc\\Validation\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  }
}
